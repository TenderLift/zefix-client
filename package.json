{
  "name": "@tenderlift/zefix-client",
  "version": "0.2.0",
  "description": "A zero-dependency, edge-compatible, and type-safe Zefix API client.",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "homepage": "https://github.com/TenderLift/zefix-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/TenderLift/zefix-client.git"
  },
  "bugs": {
    "url": "https://github.com/TenderLift/zefix-client/issues"
  },
  "keywords": [
    "zefix",
    "api",
    "client",
    "typescript",
    "edge",
    "cloudflare",
    "workers"
  ],
  "author": "TenderLift <support@tenderlift.ch>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "NOTICE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "gen": "openapi-ts -f openapi-ts.config.ts",
    "lint": "xo",
    "lint:fix": "xo --fix",
    "test": "pnpm test:node && pnpm test:workers && pnpm test:e2e",
    "test:node": "vitest run -c vitest.config.node.ts",
    "test:workers": "vitest run -c vitest.config.worker.ts",
    "test:e2e": "vitest run -c vitest.config.node.ts test/zefix-api.e2e.test.ts",
    "test:types": "tsd",
    "test:watch": "vitest",
    "size": "size-limit",
    "analyze": "pnpm build && size-limit --why",
    "prepare": "lefthook install",
    "prepublishOnly": "pnpm typecheck && pnpm build",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish"
  },
  "tsd": {
    "directory": "test"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@cloudflare/vitest-pool-workers": "^0.8.71",
    "@cloudflare/workers-types": "^4.20250909.0",
    "@hey-api/openapi-ts": "^0.82.5",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^24.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^16.4.5",
    "lefthook": "^1.12.3",
    "miniflare": "^4.20250902.0",
    "msw": "^2.11.1",
    "size-limit": "11.2.0",
    "tsd": "^0.33.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "xo": "^1.2.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.14.0"
}
