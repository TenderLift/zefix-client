#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ruzicic/Projects/tenderlift/node_modules/.pnpm/vitest@2.1.9_@types+node@20.19.11_lightningcss@1.30.1_msw@2.11.1_@types+node@20.19.11_typescript@5.9.2__terser@5.43.1/node_modules/vitest/node_modules:/Users/ruzicic/Projects/tenderlift/node_modules/.pnpm/vitest@2.1.9_@types+node@20.19.11_lightningcss@1.30.1_msw@2.11.1_@types+node@20.19.11_typescript@5.9.2__terser@5.43.1/node_modules:/Users/ruzicic/Projects/tenderlift/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ruzicic/Projects/tenderlift/node_modules/.pnpm/vitest@2.1.9_@types+node@20.19.11_lightningcss@1.30.1_msw@2.11.1_@types+node@20.19.11_typescript@5.9.2__terser@5.43.1/node_modules/vitest/node_modules:/Users/ruzicic/Projects/tenderlift/node_modules/.pnpm/vitest@2.1.9_@types+node@20.19.11_lightningcss@1.30.1_msw@2.11.1_@types+node@20.19.11_typescript@5.9.2__terser@5.43.1/node_modules:/Users/ruzicic/Projects/tenderlift/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
